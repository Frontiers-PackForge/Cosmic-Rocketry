  "description": "A Simple Addon mod to add extra rocket tiers to Ad Astra",
  "authors": [
    "MrQuentinet"
  ],
  "contributors": [],
  "contact": {
    "website": "https://modrinth.com/mod/cosmicrocketry",
    "repo": "https://github.com/Frontiers-PackForge/CosmicRocketry",
    "issues": "https://github.com/Frontiers-PackForge/CosmicRocketry/issues"
  },
  "license": "All rights reserved",
  "icon": "icon.png",
